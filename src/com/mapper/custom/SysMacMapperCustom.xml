<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.celix.wine.mapper.custom.SysMacMapperCustom">
	<select id="findSysMacByMac" parameterType="java.lang.String" resultType="com.celix.wine.po.custom.SysMacCustom">
